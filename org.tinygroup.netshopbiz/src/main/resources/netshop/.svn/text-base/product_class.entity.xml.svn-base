<entity-model id="productClass" name="productClass" title="产品分类表"
	enable-delete="true" enable-modity="true" version="2.0" cache-enabled="true">
	<operations>
		<operation id="productClassmodelopadduuid" name="addProduct_class"
			title="新增" enable-delete="true" enable-modity="true" type="add"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="class_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="addopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="class_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="class_code_uuid" />	
					<operation-field editable="true" hidden="false"
						field-id="class_name_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="productClassmodelopupdateuuid" name="modifyProduct_class"
			title="修改" enable-delete="true" enable-modity="true" type="modify"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="class_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="modifyopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="class_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="class_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="class_code_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="productClassmodelopdeleteuuid" name="deleteProduct_class"
			title="删除" enable-delete="true" enable-modity="true" type="delete"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="class_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="deleteopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="class_uuid_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="productClassmodelopqueryuuid" name="viewProduct_class"
			title="查看" enable-delete="true" enable-modity="true" type="view"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="class_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="viewopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="class_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="class_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="class_code_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="productClassmodelopaddcopyuuid" name="copyAddProduct_class"
			title="拷贝添加" enable-delete="true" enable-modity="true" type="copyAdd"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="class_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="copyAddopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="true" hidden="false"
						field-id="class_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="class_code_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
	</operations>
	<views>
		<view id="productClassmodeloptableuuid" name="tableproduct_class"
			title="表格查询" enable-delete="true" enable-modity="true" type="table"
			page-size="10" allow-edit="false" allow-filter-front="false"
			fixed-size="false" modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="class_name_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="class_code_uuid" connect-mode="AND" compare-mode="containsWith" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="class_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="class_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="class_code_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
			<references>
				<operation-reference type="operation"
					id="productClassmodelopadduuid" />
				<operation-reference type="operation"
					id="productClassmodelopaddcopyuuid" />
				<operation-reference type="operation"
					id="productClassmodelopdeleteuuid" />
				<operation-reference type="operation"
					id="productClassmodelopupdateuuid" />
				<operation-reference type="operation"
					id="productClassmodelopqueryuuid" />
			</references>
		</view>
		<view id="queryTopProductClass" name="queryTopProductClass"
			title="顶层产品分类查询" enable-delete="true" enable-modity="true" type="table"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="class_code_uuid" connect-mode="AND" compare-mode="lengthEquals" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="class_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="class_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="class_code_uuid" aggregate-by-view="false" />	
				</view-group>
			</view-groups>
			<order-fields>
				<order-field field-id="class_code_uuid" order-mode="asc"
					editable="false" hidden="false" />
			</order-fields>
		</view>
		<view id="productClassTree" name="productClassTree" title="产品分类树"
			type="treeWithCode" width="400" height="300" modal="false" allow-edit="true">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="class_code_uuid" compare-mode="beginsWith">
				</condition-field>
				<condition-field editable="true" hidden="false"
					field-id="class_code_uuid" compare-mode="lengthEquals">
				</condition-field>
			</condition-fields>
			<view-groups>
				<view-group>
					<display-field field-id="class_uuid_uuid" hidden="true" />
					<display-field field-id="class_name_uuid" hidden="false" />
					<display-field field-id="class_code_uuid" hidden="false" />
				</view-group>
			</view-groups>
			<order-fields>
				<order-field field-id="class_code_uuid" order-mode="asc"
					editable="false" hidden="false" />
			</order-fields>
			<extendInformation>
				<![CDATA[
				<extend-info>
				  <field-maps>
					<field-map name="text" fieldId="class_name_uuid" pattern=""/>
					<field-map name="value" fieldId="class_code_uuid" pattern=""/>
					<field-map name="id" fieldId="class_uuid_uuid" pattern=""/>
					<field-map name="navigateUrl" fieldId="" pattern="javscript:openUrl('${class_uuid_uuid}')"/>
				  </field-maps>
				</extend-info>
				]]>
			</extendInformation>
		</view>
	</views>
	<groups>
		<group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<field standard-field-id="class_uuid" primary="true" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="class_uuid_uuid" />
			<field standard-field-id="class_code" primary="false" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="class_code_uuid" />	
			<field standard-field-id="class_name" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="class_name_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="产品分类名称不能为空" />
				</validate-rules>
			</field>
		</group>
	</groups>
</entity-model>