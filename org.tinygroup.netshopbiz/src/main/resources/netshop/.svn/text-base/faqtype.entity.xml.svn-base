	enable-delete="true" enable-modity="true" version="2.0" cache-enabled="true">
	<operations>
		<operation id="faqtypemodelopadduuid" name="addfaqtype"
			title="新增" enable-delete="true" enable-modity="true" type="add"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="faqtype_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="addopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="faqtype_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faqtype_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faqtype_order_number_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faqtype_parent_uuid">
						<input-mode class="org.tinygroup.entity.common.TreeInputMode"
							type="treeMode" url="EntityModel_tree_faqtype_faqTypeTree.modeljson"
							tree-id="faqtypeTree1" param-info=""
							popup-info="showEffect: 'blind',hideEffect: 'blind'"
							tree-info="showCheckBoxes: false, 
                     expandAnimation: { effect: 'blind', duration: 500 }, 
                     collapseAnimation: { effect: 'puff', duration: 500 }">
						</input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="faqtype_description_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="faqtypemodelopupdateuuid" name="modifyfaqtype"
