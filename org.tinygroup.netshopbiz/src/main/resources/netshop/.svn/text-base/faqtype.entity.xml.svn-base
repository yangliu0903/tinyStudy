	<views>
		<view id="faqtypemodeloptableuuid" name="tablefaqtype" title="表格查询"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="faqtype_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faqtype_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faqtype_parent_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faqtype_order_number_uuid" aggregate-by-view="false" />	
				</view-group>
			</view-groups>
			<references>
				<operation-reference type="operation"
					id="faqtypemodelopadduuid" />
				<operation-reference type="operation"
					id="faqtypemodelopaddcopyuuid" />
				<operation-reference type="operation"
					id="faqtypemodelopdeleteuuid" />
				<operation-reference type="operation"
					id="faqtypemodelopupdateuuid" />
				<operation-reference type="operation"
					id="faqtypemodelopqueryuuid" />
			</references>
		</view>
		<view id="faqTypeTree" name="faqTypeTree" title="分类树" type="tree"
			width="400" height="300" pageSize="10" modal="false" allow-edit="true">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="faqtype_parent_uuid" compare-mode="equals">
				</condition-field>
			</condition-fields>
			<view-groups>
				<view-group>
					<display-field field-id="faqtype_uuid_uuid" hidden="true" />
					<display-field field-id="faqtype_name_uuid" hidden="false" />
					<display-field field-id="faqtype_description_uuid"
						hidden="false" />
				</view-group>
			</view-groups>
			<order-fields>
				<order-field field-id="faqtype_uuid_uuid" order-mode="asc"
					editable="false" hidden="false" />
			</order-fields>
			<extendInformation>
				<![CDATA[
				<extend-info>
				  <field-maps>
					<field-map name="text" fieldId="faqtype_name_uuid" pattern=""/>
					<field-map name="value" fieldId="faqtype_uuid_uuid" pattern=""/>
					<field-map name="navigateUrl" fieldId="" pattern="javscript:openUrl('${faqtype_uuid_uuid}')"/>
				  </field-maps>
				</extend-info>
				]]>
			</extendInformation>
		</view>
	</views>
	<groups>
		<group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<field standard-field-id="faqtype_uuid" primary="true" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="faqtype_uuid_uuid" />
			<field standard-field-id="faqtype_name" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="faqtype_name_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="faq分类名称不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="faqtype_description" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true"
				id="faqtype_description_uuid" />
			<field standard-field-id="faqtype_parent" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="faqtype_parent_uuid" />
			<field standard-field-id="faqtype_order_number" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true"
				id="faqtype_order_number_uuid" />
		</group>
	</groups>
</entity-model>