<entity-model id="faq" name="faq" title="FAQ"
	enable-delete="true" enable-modity="true" version="2.0" cache-enabled="true">
	<operations>
		<operation id="faqmodelopadduuid" name="addFaq" title="新增"
			enable-delete="true" enable-modity="true" type="add" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="faq_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="addopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="faq_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faqtype_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_order_number_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_question_uuid">
						<input-mode type="textWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="faq_answer_uuid">
						<input-mode type="simpleEditorTopMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="faqmodelopupdateuuid" name="modifyFaq" title="修改"
			enable-delete="true" enable-modity="true" type="modify" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="faq_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="modifyopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="faq_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faqtype_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_order_number_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_question_uuid">
						<input-mode type="textWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="faq_answer_uuid">
						<input-mode type="simpleEditorTopMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="faqmodelopdeleteuuid" name="deleteFaq" title="删除"
			enable-delete="true" enable-modity="true" type="delete" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="faq_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="deleteopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="faq_uuid_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="faqmodelopqueryuuid" name="viewFaq" title="查看"
			enable-delete="true" enable-modity="true" type="view" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="faq_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="viewopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="true" hidden="true"
						field-id="faq_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faqtype_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_order_number_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_question_uuid">
						<input-mode type="labelWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="faq_answer_uuid">
						<input-mode type="labelWholeLineMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="faqmodelopaddcopyuuid" name="copyAddFaq"
			title="拷贝添加" enable-delete="true" enable-modity="true" type="copyAdd"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="faq_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="copyAddopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="true" hidden="false"
						field-id="faqtype_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_order_number_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="faq_question_uuid">
						<input-mode type="textWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="faq_answer_uuid">
						<input-mode type="simpleEditorTopMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
	</operations>
	<views>
		<view id="faqmodeloptableuuid" name="tablefaq" title="表格查询"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="faq_question_uuid" connect-mode="AND" compare-mode="containsWith" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="faq_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faqtype_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faq_question_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faq_order_number_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
			<references>
				<operation-reference type="operation" id="faqmodelopadduuid" />
				<operation-reference type="operation"
					id="faqmodelopaddcopyuuid" />
				<operation-reference type="operation" id="faqmodelopdeleteuuid" />
				<operation-reference type="operation" id="faqmodelopupdateuuid" />
				<operation-reference type="operation" id="faqmodelopqueryuuid" />
			</references>
		</view>

		<view id="queryFaqByType" name="queryFaqByType" title="根据分类查询faq"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="faqtype_uuid_uuid" connect-mode="AND" compare-mode="equals" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="false"
						field-id="faq_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faqtype_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faq_question_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faq_order_number_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="faq_answer_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
		</view>
	</views>
	<groups>
		<group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<field standard-field-id="faq_uuid" primary="true" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="faq_uuid_uuid" />
			<field standard-field-id="faqtype_uuid" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="faqtype_uuid_uuid" />
			<field standard-field-id="faq_question" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="faq_question_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="faq问题不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="faq_answer" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="faq_answer_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="faq答案不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="faq_order_number" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="faq_order_number_uuid" />
		</group>
	</groups>
</entity-model>