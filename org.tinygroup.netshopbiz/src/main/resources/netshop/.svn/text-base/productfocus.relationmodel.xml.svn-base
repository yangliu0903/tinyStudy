<relation-model id="userProductFocusRelation" name="userProductFocusRelation" title="用户产品关注关联查询" main-bean-type="user">
	<views>
		<view id="queryUserProductFocus" name="queryUserProductFocus" title="查询用户产品关注信息"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" compare-mode="equals" />
				<condition-field editable="true" hidden="false"
					field-id="focus_target_uuid" connect-mode="AND" compare-mode="equals" />	
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="false"
						field-id="user_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="true"
						field-id="focus_target_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="product_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="product_fare_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="online_time_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="product_stock_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="product_delivery_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="unit_price_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="product_norm_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_description_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_detial_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_selled_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_modify_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_discount_uuid" aggregate-by-view="false" />	
					<display-field editable="false" hidden="false"
						field-id="product_priority_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="picture_uuid_uuid" aggregate-by-view="false" />		
				</view-group>
			</view-groups>
		</view>
	</views>
	
	<model-references>
	    <model-reference relation-id="userProductFocusRelation" />
	</model-references>
	
	<relation-groups>
		<relation-group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<relation-field   field-id="user_uuid_uuid" model-id="userFocus"/>
			<relation-field   field-id="focus_target_uuid" model-id="userFocus"/>
			<relation-field   field-id="focus_target_uuid_uuid" model-id="userFocus"/>
			<relation-field   field-id="product_uuid_uuid" model-id="product"/>
			<relation-field   field-id="product_name_uuid" model-id="product"/>
			<relation-field   field-id="product_fare_uuid" model-id="product"/>
			<relation-field   field-id="online_time_uuid" model-id="product"/>
			<relation-field   field-id="product_stock_uuid" model-id="product"/>
			<relation-field   field-id="product_delivery_uuid" model-id="product"/>
			<relation-field   field-id="unit_price_uuid" model-id="product"/>
			<relation-field   field-id="product_norm_uuid" model-id="product"/>
			<relation-field   field-id="product_description_uuid" model-id="product"/>
			<relation-field   field-id="product_detial_uuid" model-id="product"/>
			<relation-field   field-id="product_selled_uuid" model-id="product"/>
			<relation-field   field-id="product_modify_uuid" model-id="product"/>
			<relation-field   field-id="picture_uuid_uuid" model-id="product"/>
			<relation-field   field-id="product_discount_uuid" model-id="product"/>
			<relation-field   field-id="product_priority_uuid" model-id="product"/>
		</relation-group>
	</relation-groups>
</relation-model>