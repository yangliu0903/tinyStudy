<entity-model id="news" name="news" title="公告"
	enable-delete="true" enable-modity="true" version="2.0" cache-enabled="true">
	<operations>
		<operation id="newsmodelopadduuid" name="addNews" title="新增"
			enable-delete="true" enable-modity="true" type="add" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="news_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="addopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="news_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_title_uuid" >
						<input-mode type="textWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_level_uuid" >
					      <input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="news_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_createtime_uuid" >
					    <input-mode type="dateMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_auth_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_content_uuid" >
					   <input-mode type="simpleEditorTopMode"></input-mode>	
					</operation-field>	
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="newsmodelopupdateuuid" name="modifyNews"
			title="修改" enable-delete="true" enable-modity="true" type="modify"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="news_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="modifyopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="news_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_title_uuid" >
						<input-mode type="textWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_level_uuid" >
					      <input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="news_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_createtime_uuid" >
					    <input-mode type="dateMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_auth_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_content_uuid" >
					   <input-mode type="simpleEditorTopMode"></input-mode>	
					</operation-field>	
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="newsmodelopdeleteuuid" name="deleteNews"
			title="删除" enable-delete="true" enable-modity="true" type="delete"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="news_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="deleteopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="news_uuid_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="newsmodelopqueryuuid" name="viewNews" title="查看"
			enable-delete="true" enable-modity="true" type="view" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="news_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="viewopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="news_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_title_uuid" >
						<input-mode type="labelWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_content_uuid" >
					   <input-mode type="labelWholeLineMode"></input-mode>	
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_level_uuid" >
					      <input-mode type="dictLabelMode"
							class="org.tinygroup.entity.common.DictInputMode" dictType="news_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_createtime_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_auth_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="newsmodelopaddcopyuuid" name="copyAddNews"
			title="拷贝添加" enable-delete="true" enable-modity="true" type="copyAdd"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="news_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="copyAddopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="true" hidden="false"
						field-id="news_title_uuid" >
						<input-mode type="textWholeLineMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_level_uuid" >
					      <input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="news_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_createtime_uuid" >
				      <input-mode type="dateMode"></input-mode>
				   </operation-field>
					<operation-field editable="true" hidden="false"
						field-id="news_auth_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="news_content_uuid" >
					   <input-mode type="simpleEditorTopMode"></input-mode>	
					</operation-field>	
				</operation-fields>
			</operation-group>
		</operation>
	</operations>
	<views>
		<view id="newsmodeloptableuuid" name="tablenews" title="表格查询"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="news_title_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="news_level_uuid" connect-mode="AND" compare-mode="containsWith" >
				 <input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="news_level"></input-mode>	
				</condition-field>
				<condition-field editable="true" hidden="false"
					field-id="news_createtime_uuid" connect-mode="AND" compare-mode="containsWith" >
				     <input-mode type="dateMode"></input-mode>	
				</condition-field>
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="news_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_title_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_level_uuid" aggregate-by-view="false" >
					      <input-mode type="dictLabelMode"
							class="org.tinygroup.entity.common.DictInputMode" dictType="news_level"></input-mode>	
					</display-field>
					<display-field editable="false" hidden="false"
						field-id="news_createtime_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_auth_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
			<references>
				<operation-reference type="operation" id="newsmodelopadduuid" />
				<operation-reference type="operation"
					id="newsmodelopaddcopyuuid" />
				<operation-reference type="operation"
					id="newsmodelopdeleteuuid" />
				<operation-reference type="operation"
					id="newsmodelopupdateuuid" />
				<operation-reference type="operation" id="newsmodelopqueryuuid" />
			</references>
		</view>
		
		<view id="queryAllNews" name="queryAllNews" title="查询所有公告"
			enable-delete="true" enable-modity="true" type="table" 
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<order-fields>
				<order-field field-id="news_createtime_uuid" order-mode="desc"
					editable="false" hidden="false" />
			</order-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="news_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_title_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_content_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_level_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_createtime_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_auth_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
		</view>
		
		<view id="queryNewsById" name="queryNewsById" title="根据公告编号查询公告"
			enable-delete="true" enable-modity="true" type="table" 
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="news_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="news_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_title_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_content_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_level_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_createtime_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="news_auth_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
		</view>
	</views>
	<groups>
		<group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<field standard-field-id="news_uuid" primary="true" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="news_uuid_uuid" />
			<field standard-field-id="news_title" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="news_title_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="公告标题不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="news_content" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="news_content_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="公告内容不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="news_level" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="news_level_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="新闻级别不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="news_createtime" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="news_createtime_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="发布时间不能为空" />
					<validate-rule rule-name="date" messege="日期格式为：YYYY-MM-DD" />
				</validate-rules>
			</field>
			<field standard-field-id="news_auth" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="news_auth_uuid" />
		</group>
	</groups>
</entity-model>