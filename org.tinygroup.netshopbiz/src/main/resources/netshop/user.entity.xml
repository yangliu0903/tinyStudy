<entity-model id="user" name="user" title="用户"
	enable-delete="true" enable-modity="true" version="2.0" cache-enabled="true">
	<operations>
		<operation id="usermodelopadduuid" name="addUser" title="新增"
			enable-delete="true" enable-modity="true" type="add" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="addopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="user_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_password_uuid">
						<input-mode type="passwordMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_birthday_uuid">
						<input-mode type="dateMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_status_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="user_status"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_role_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="user_role"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_description_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="usermodelopupdateuuid" name="modifyUser"
			title="修改" enable-delete="true" enable-modity="true" type="modify"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="modifyopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="user_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_password_uuid">
						<input-mode type="passwordMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_birthday_uuid">
						<input-mode type="dateMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_status_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="user_status"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_role_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="user_role"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_description_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>

		<operation id="modifyPassword" name="modifyPassword" title="修改密码"
			enable-delete="true" enable-modity="true" type="modify" fixed-size="true"
			modal="true" width="330" height="190">
			<customize-stage-configs>
				<customize-stage-config stage-name="save"
					service-processor-bean="modifyPasswordService" view-path="/page/modify_save.page"></customize-stage-config>
			</customize-stage-configs>
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="modifyopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="user_uuid_uuid" />
					<operation-field editable="false" hidden="true"
						field-id="user_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="old_password">
						<input-mode type="passwordMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="new_password">
						<input-mode type="passwordMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="confirm_password">
						<input-mode type="passwordMode"></input-mode>
					</operation-field>

				</operation-fields>
			</operation-group>
		</operation>

		<operation id="usermodelopdeleteuuid" name="deleteUser"
			title="删除" enable-delete="true" enable-modity="true" type="delete"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="deleteopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="user_uuid_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="usermodelopqueryuuid" name="viewUser" title="查看"
			enable-delete="true" enable-modity="true" type="view" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="viewopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="user_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_password_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_description_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_birthday_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_status_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_role_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="usermodelopaddcopyuuid" name="copyAddUser"
			title="拷贝添加" enable-delete="true" enable-modity="true" type="copyAdd"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="copyAddopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="true" hidden="false"
						field-id="user_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_password_uuid">
						<input-mode type="passwordMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_birthday_uuid">
						<input-mode type="dateMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_status_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="user_status"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_role_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="user_role"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="user_description_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
	</operations>
	<views>
		<view id="usermodeloptableuuid" name="tableuser" title="表格查询"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="user_id_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="user_name_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="user_birthday_uuid">
					<input-mode type="dateMode"></input-mode>
				</condition-field>
				<condition-field editable="true" hidden="false"
					field-id="user_status_uuid">
					<input-mode type="dictComboMode "
						class="org.tinygroup.entity.common.DictInputMode" dictType="user_status"></input-mode>
				</condition-field>
				<condition-field editable="true" hidden="false"
					field-id="user_role_uuid">
					<input-mode type="dictComboMode "
						class="org.tinygroup.entity.common.DictInputMode" dictType="user_role"></input-mode>
				</condition-field>
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="user_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_id_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_birthday_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_status_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_role_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
			<references>
				<operation-reference type="operation" id="usermodelopadduuid" />
				<operation-reference type="operation"
					id="usermodelopaddcopyuuid" />
				<operation-reference type="operation" id="modifyPassword" />
				<operation-reference type="operation"
					id="usermodelopdeleteuuid" />
				<operation-reference type="operation"
					id="usermodelopupdateuuid" />
				<operation-reference type="operation" id="usermodelopqueryuuid" />
			</references>
		</view>

		<view id="queryUserById" name="queryUserById" title="根据用户编号查询用户信息"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" compare-mode="equals" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="user_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_id_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_password_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_description_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_birthday_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_status_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_role_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
		</view>
	</views>
	<groups>
		<group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<field standard-field-id="user_uuid" primary="true" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="user_uuid_uuid" />
			<field standard-field-id="user_id" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="user_id_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="用户编号不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="user_name" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="user_name_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="用户名称不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="user_password" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="user_password_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="用户密码不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="old_password" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="false" id="old_password">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="旧密码不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="new_password" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="false" id="new_password">
				<validate-rules>
					<validate-rule rule-name="equalTo" rule-value="confirmPassword"
						messege="新密码与确认密码输入不相同" />
					<validate-rule rule-name="required" rule-value=""
						messege="新密码不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="confirm_password" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="false" id="confirm_password">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="确认密码不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="user_description" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="user_description_uuid" />
			<field standard-field-id="user_birthday" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="user_birthday_uuid">
				<validate-rules>
					<validate-rule rule-name="date" messege="日期格式为：YYYY-MM-DD" />
				</validate-rules>
			</field>
			<field standard-field-id="user_status" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="user_status_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="用户状态不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="user_role" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="user_role_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="用户角色不能为空" />
				</validate-rules>
			</field>
		</group>
	</groups>
</entity-model>