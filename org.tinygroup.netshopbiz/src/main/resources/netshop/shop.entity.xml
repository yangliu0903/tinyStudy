<entity-model id="shop" name="shop" title="店铺信息"
	enable-delete="true" enable-modity="true" version="2.0" cache-enabled="true">
	<operations>
		<operation id="shopmodelopadduuid" name="addShop" title="新增"
			enable-delete="true" enable-modity="true" type="add" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="shop_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="addopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="shop_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_level_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="shop_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="picture_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_status_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_summary_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="shop_description_uuid">
						<input-mode type="fullEditorTopMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="shopmodelopupdateuuid" name="modifyShop"
			title="修改" enable-delete="true" enable-modity="true" type="modify"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="shop_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="modifyopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="shop_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_level_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="shop_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="picture_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_status_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_summary_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="shop_description_uuid">
						<input-mode type="fullEditorTopMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="shopmodelopdeleteuuid" name="deleteShop"
			title="删除" enable-delete="true" enable-modity="true" type="delete"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="shop_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="deleteopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="shop_uuid_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="shopmodelopqueryuuid" name="viewShop" title="查看"
			enable-delete="true" enable-modity="true" type="view" fixed-size="false"
			modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="shop_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="viewopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="false" hidden="true"
						field-id="shop_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_level_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_summary_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_description_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="picture_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_status_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_uuid_uuid" />
				</operation-fields>
			</operation-group>
		</operation>
		<operation id="shopmodelopaddcopyuuid" name="copyAddShop"
			title="拷贝添加" enable-delete="true" enable-modity="true" type="copyAdd"
			fixed-size="false" modal="true">
			<condition-fields>
				<condition-field editable="false" hidden="false"
					field-id="shop_uuid_uuid" connect-mode="AND" />
			</condition-fields>
			<operation-group id="copyAddopbasicgroup" name="basicgroup"
				title="基本信息" enable-delete="true" enable-modity="true">
				<operation-fields>
					<operation-field editable="true" hidden="false"
						field-id="shop_name_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_id_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_level_uuid">
						<input-mode type="dictComboMode "
							class="org.tinygroup.entity.common.DictInputMode" dictType="shop_level"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="picture_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_status_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="user_uuid_uuid" />
					<operation-field editable="true" hidden="false"
						field-id="shop_summary_uuid">
						<input-mode type="textAreaLeftMode"></input-mode>
					</operation-field>
					<operation-field editable="true" hidden="false"
						field-id="shop_description_uuid">
						<input-mode type="fullEditorTopMode"></input-mode>
					</operation-field>
				</operation-fields>
			</operation-group>
		</operation>
	</operations>
	<views>
		<view id="shopmodeloptableuuid" name="tableshop" title="表格查询"
			enable-delete="true" enable-modity="true" type="table" page-size="10"
			allow-edit="false" allow-filter-front="false" fixed-size="false"
			modal="false" front-paging="false">
			<condition-fields>
				<condition-field editable="true" hidden="false"
					field-id="shop_name_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="shop_id_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="shop_level_uuid" connect-mode="AND" compare-mode="containsWith">
					<input-mode type="dictComboMode "
						class="org.tinygroup.entity.common.DictInputMode" dictType="shop_level"></input-mode>
				</condition-field>
				<condition-field editable="true" hidden="false"
					field-id="shop_status_uuid" connect-mode="AND" compare-mode="containsWith" />
				<condition-field editable="true" hidden="false"
					field-id="user_uuid_uuid" connect-mode="AND" compare-mode="containsWith" />
			</condition-fields>
			<view-groups>
				<view-group id="viewbasicgroup" name="basicgroup" title="基本信息"
					enable-delete="true" enable-modity="true">
					<display-field editable="false" hidden="true"
						field-id="shop_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="shop_name_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="true"
						field-id="picture_uuid_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="shop_id_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="shop_level_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="shop_status_uuid" aggregate-by-view="false" />
					<display-field editable="false" hidden="false"
						field-id="user_uuid_uuid" aggregate-by-view="false" />
				</view-group>
			</view-groups>
			<references>
				<operation-reference type="operation" id="shopmodelopadduuid" />
				<operation-reference type="operation"
					id="shopmodelopaddcopyuuid" />
				<operation-reference type="operation"
					id="shopmodelopdeleteuuid" />
				<operation-reference type="operation"
					id="shopmodelopupdateuuid" />
				<operation-reference type="operation" id="shopmodelopqueryuuid" />
			</references>
		</view>
	</views>
	<groups>
		<group id="basicgroup" name="basicgroup" title="基本信息"
			enable-delete="true" enable-modity="true">
			<field standard-field-id="shop_uuid" primary="true" unique="true"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="shop_uuid_uuid" />
			<field standard-field-id="shop_name" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="shop_name_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="店铺名称不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="shop_id" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="shop_id_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="店铺编号不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="shop_level" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="shop_level_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="店铺等级不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="shop_summary" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="shop_summary_uuid" />
			<field standard-field-id="shop_description" primary="false"
				unique="false" display="false" not-null="false" auto-increase="false"
				editable="false" hidden="false" table-field="true" id="shop_description_uuid" />
			<field standard-field-id="picture_uuid" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="picture_uuid_uuid" />
			<field standard-field-id="shop_status" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="shop_status_uuid">
				<validate-rules>
					<validate-rule rule-name="required" rule-value=""
						messege="店铺状态不能为空" />
				</validate-rules>
			</field>
			<field standard-field-id="user_uuid" primary="false" unique="false"
				display="false" not-null="false" auto-increase="false" editable="false"
				hidden="false" table-field="true" id="user_uuid_uuid" />
		</group>
	</groups>
</entity-model>