<flow id="business" enable="true" private-context="false">
  <nodes>
    <node id="start" name="start" title="开始">
      <next-nodes>
        <next-node next-node-id="helloworld"/>
      </next-nodes>
    </node>
    <node id="helloworld" name="helloworld" title="HelloWorld组件">
      <component name="helloworld" title="HelloWorld组件">
        <properties>
          <flow-property name="name" type="java.lang.String" value="${name}"/>
          <flow-property name="result" type="java.lang.String" value="result"/>
        </properties>
      </component>
      <next-nodes>
        <next-node next-node-id="end"/>
      </next-nodes>
    </node>
    <node id="end" name="end" title="结束">
      <next-nodes/>
    </node>
  </nodes>
</flow>