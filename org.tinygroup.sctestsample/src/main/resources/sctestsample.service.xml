<service-components>
	<service-component type="org.tinygroup.sctestsample.Hello">
		<service-method local-name="hello" service-id="hello"
			method-name="hello" cacheable="false">
			<service-parameters>
				<service-parameter name="name" type="java.lang.String"
					required="false" is-array="false" />
			</service-parameters>
			<service-result name="result" required="true"
				is-array="false" type="java.lang.String" />
		</service-method>
		
		<service-method local-name="userhello" service-id="userhello"
