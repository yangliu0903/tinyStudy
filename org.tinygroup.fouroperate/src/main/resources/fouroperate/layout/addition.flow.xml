<flow id="addition" name="addition" enable="false" private-context="false">
  <nodes>
    <node id="start" name="start" title="开始">
      <next-nodes>
        <next-node next-node-id="addition"/>
      </next-nodes>
    </node>
    <node id="addition" name="addition" title="加法服务">
      <component name="additionComponent">
        <properties>
          <flow-property name="number1" value="${number1}"/>
          <flow-property name="number2" value="${number2}"/>
          <flow-property name="resultKey" value="result"/>
        </properties>
      </component>
      <next-nodes>
        <next-node next-node-id="end"/>
      </next-nodes>
    </node>
    <node id="end" name="end" title="结束">
      <next-nodes/>
    </node>
  </nodes>
</flow>