<flow id="fouroperate" name="fouroperate" begin-node-id="addition"
	private-context="false" enable="false">
	<nodes>
	<node id="addition" name="addition"  title="加法服务">
			<component name="additionComponent">
				<properties>
					<flow-property name="number1" value="${number1}" />
					<flow-property name="number2" value="${number2}" />
					<flow-property name="resultKey" value="result" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="writeText"></next-node>
			</next-nodes>
		</node>
		<node id="subtraction" name="subtraction"  title="减法服务">
			<component name="subtractionComponent">
				<properties>
					<flow-property name="number1" value="${number1}" />
					<flow-property name="number2" value="${number2}" />
					<flow-property name="resultKey" value="result" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="writeText"></next-node>
			</next-nodes>
		</node>
		<node id="multi" name="multi"  title="乘法服务">
			<component name="multiComponent">
				<properties>
					<flow-property name="number1" value="${number1}" />
					<flow-property name="number2" value="${number2}" />
					<flow-property name="resultKey" value="result" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="writeText"></next-node>
			</next-nodes>
		</node>
		<node id="division" name="division"  title="除法服务">
			<component name="divisionComponent">
				<properties>
					<flow-property name="number1" value="${number1}" />
					<flow-property name="number2" value="${number2}" />
					<flow-property name="resultKey" value="result" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="writeText"></next-node>
			</next-nodes>
		</node>
		<node id="writeText" name="writeText" title="写入内容"
			next-node-id="end">
			<component name="writeTextComponent">
				<properties>
					<flow-property name="resultKey" value="result" />
				</properties>
			</component>
			<next-nodes>
				<next-node next-node-id="end">
				</next-node>
			</next-nodes>
		</node>
	</nodes>
</flow>
