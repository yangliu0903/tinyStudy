		<placeholder-list>
			<placeholder name="l" title="长度">
				<description>字符串的长度</description>
			</placeholder>
		</placeholder-list>
		<dialect-type-list>
			<dialect-type language="oracle" type="CLOB"
				default-value="">
			</dialect-type>
			<dialect-type language="mysql" type="LONGTEXT"
				default-value="">
			</dialect-type>
			<dialect-type language="java" type="String"
				default-value="">
			</dialect-type>
		</dialect-type-list>
	</standard-type>
	<standard-type type="Blob" id="type_blob">
