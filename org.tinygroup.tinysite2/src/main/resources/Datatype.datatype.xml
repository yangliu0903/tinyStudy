<standard-types package-name="org.tinygroup">
	<standard-type type="TinyText" id="type_tiny_text">
		<dialect-type-list>
			<dialect-type language="oracle" type="VARCHAR2(255)"
				default-value="">
			</dialect-type>
			<dialect-type language="mysql" type="TINYTEXT"
				default-value="">
			</dialect-type>
			<dialect-type language="java" type="String"
				default-value="">
			</dialect-type>
		</dialect-type-list>
	</standard-type>
	<standard-type type="Text" id="type_text">
		<dialect-type-list>
			<dialect-type language="oracle" type="CLUB"
				default-value="">
			</dialect-type>
			<dialect-type language="mysql" type="TEXT"
				default-value="">
			</dialect-type>
