		</dialect-type-list>
	</standard-type>
	<standard-type type="Char" id="type_char">
		<placeholder-list>
			<placeholder name="length" title="长度">
				<description>字符串的长度</description>
			</placeholder>
		</placeholder-list>
		<dialect-type-list>
			<dialect-type language="oracle" type="CHAR(${length})"
				default-value="">
			</dialect-type>
			<dialect-type language="mysql" type="CHAR(${length})"
				default-value="">
			</dialect-type>
			<dialect-type language="derby" type="CHAR(${length})"
				default-value="">
			</dialect-type>
			<dialect-type language="java" type="String"
				default-value="">
			</dialect-type>
		</dialect-type-list>
	</standard-type>
	<standard-type type="Varchar" id="type_varchar">
		<placeholder-list>
			<placeholder name="length" title="长度">
				<description>字符串的长度</description>
			</placeholder>
		</placeholder-list>
		<dialect-type-list>
			<dialect-type language="oracle" type="VARCHAR2(${length})"
				default-value="">
			</dialect-type>
			<dialect-type language="mysql" type="VARCHAR(${length})"
				default-value="">
			</dialect-type>
				<dialect-type language="derby" type="VARCHAR(${length})"
					default-value="">
				</dialect-type>
				<dialect-type language="java" type="String"
					default-value="">
