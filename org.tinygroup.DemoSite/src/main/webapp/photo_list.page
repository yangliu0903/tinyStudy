<?xml version="1.0" encoding="utf-8"?>
<photos>

	<config 
		auto_play="true" 
		auto_play_duration="2.4" 
		grid_row="4" 
		grid_column="10" 
		tween_duration="0.64" 
		tween_delay="0.015"
		stylesheet="#link('/css/slider_style.css')"
		bar_status="1">
	</config>
	
	<photo>
		<filename>#link("/images/slider/photo_780x210_01.jpg")</filename>
		<description></description>
		<transition flow="in" direction="right" rotation="180"></transition>
	</photo>
	
	<photo>
		<filename>#link("/images/slider/photo_780x210_02.jpg")</filename>
		<description></description>
		<transition flow="in" direction="up" rotation="-180"></transition>
	</photo>
	
	<photo>
		<filename>#link("/images/slider/photo_780x210_03.jpg")</filename>
		<description></description>
		<transition flow="out" direction="left" rotation="-180"></transition>
	</photo>
	
	<photo>
		<filename>#link("/images/slider/photo_780x210_04.jpg")</filename>
		<description></description>
		<transition flow="out" direction="up" rotation="90"></transition>
	</photo>
	
	<photo>
		<filename>#link("/images/slider/photo_780x210_05.jpg")</filename>
		<description></description>
		<transition flow="in" direction="down" rotation="180"></transition>
	</photo>
	
</photos>