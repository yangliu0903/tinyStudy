   // 更换章节时的操作
function changeChapter(){
  var obj = document.getElementById("chapter"); //selectid
  var index = obj.selectedIndex; // 选中选项的索引
  var chp = obj.options[index].value; // 选中选项对应的文本 
  
  var sample =  document.getElementById("sample");
  sample.length=0;
  var all = json[chp];
  for(var ch in all){
     var varItem = new Option(all[ch],ch);      
         sample.options.add(varItem);     
  }
  changeSample(chp+"_0");

}

function changeSample(chp){
  var sample = null;
  if(typeof(chp) != "undefined" ){
  
    sample = chp;
  }else{
    var obj = document.getElementById("sample"); //selectid
    var index = obj.selectedIndex; // 选中索引
    sample = obj.options[index].value; // 选中文本  
  }
  
  var template = document.getElementById(sample+"_template").innerHTML;
  var model = document.getElementById(sample+"_model").innerHTML;
  setContent("input",replaceContent(template));
  setContent("json",model);
  
  
}
// 文本替换，去掉文本标志,\#[[标志可以在服务里面去掉
function replaceContent(template){
  //var startStatment =  "<"+"%";// getContent("startStatment");
  //var endStatment = "%"+">";//getContent("endStatment");
  //var startHolder = "$"+"{" //getContent("startHolder");
  //var endHolder = "}"//getContent("endHolder");
  //template = template.replaceAll(_ss, startStatment);
  //template = template.replaceAll(_es, endStatment);
  //template = template.replaceAll(_sh, startHolder);
  //template = template.replaceAll(_eh, endHolder);

  template = template.replaceAll("&lt;","<");
  template = template.replaceAll("&gt;",">");
  
  return template
}


function getContent(id){
  return document.getElementById(id).value;
}
function setContent(id,content){
  document.getElementById(id).value = content;;
}

function validateForm(){
  var input = getContent("input");
  if(input.length>5000){
    alert("模板输出内容太长");
    return false ;
  }
  
  var json = getContent("json");
  if(json.length>1000){
    alert("模型内容太长");
    return false ;
  }
}

