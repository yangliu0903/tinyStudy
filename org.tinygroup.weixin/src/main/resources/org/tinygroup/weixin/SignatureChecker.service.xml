<service-components>
  <service-component type="org.tinygroup.weixin.SignatureChecker" bean="signatureChecker">
    <service-method local-name="checkSignature" service-id="checkSignature" method-name="checkSignature" cacheable="false">
      <service-parameters>
        <service-parameter name="timestamp" type="java.lang.String" required="false" is-array="false"/>
        <service-parameter name="nonce" type="java.lang.String" required="false" is-array="false"/>
        <service-parameter name="signature" type="java.lang.String" required="false" is-array="false"/>
        <service-parameter name="echostr" type="java.lang.String" required="false" is-array="false"/>
        <service-parameter name="tinyRequestRemoteHost" type="java.lang.String" required="false" is-array="false"/>
        <service-parameter name="postData" type="java.lang.Object" required="false" is-array="false"/>
      </service-parameters>
      <service-result name="checkSignatureResult" required="false" is-array="false" type="java.lang.String"/>
    </service-method>
  </service-component>
</service-components>