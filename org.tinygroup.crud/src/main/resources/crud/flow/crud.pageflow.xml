<flow id="crud" name="crud" begin-node-id="queryUsers"
	private-context="false" enable="false">
	<nodes>
	 <node id="addUser" name="addUser"  title="添加用户">
			<component name="callService">
				<properties>
					<flow-property name="serviceId" value="addUserWithFlow" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="redirect"></next-node>
			</next-nodes>
		</node>
		<node id="updateUser" name="updateUser"  title="更新用户">
			<component name="callService">
				<properties>
					<flow-property name="serviceId" value="updateUserWithFlow" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="redirect"></next-node>
			</next-nodes>
		</node>
		<node id="deleteUser" name="deleteUser"  title="删除用户">
			<component name="callService">
				<properties>
					<flow-property name="serviceId" value="deleteUserWithFlow" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="redirect"></next-node>
			</next-nodes>
		</node>
		<node id="queryUsers" name="queryUsers"  title="查询用户">
			<component name="callService">
				<properties>
					<flow-property name="serviceId" value="queryUsersWithFlow" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="list"></next-node>
			</next-nodes>
		</node>
		<node id="queryUserById" name="queryUserById"  title="根据主键查询用户">
			<component name="callService">
				<properties>
					<flow-property name="serviceId" value="queryUserByIdWithFlow" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="operate"></next-node>
			</next-nodes>
		</node>
		<node id="list" name="list" title="转化页面">
			<component name="forwardComponent">
				<properties>
					<flow-property name="path" value="/crud/flow/list.page" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="end"></next-node>
			</next-nodes>
		</node>
		<node id="redirect" name="redirect" title="重定向页面">
			<component name="redirectComponent">
				<properties>
					<flow-property name="path" value="crud.pageflow?tiny_flow_id=queryUsers" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="end"></next-node>
			</next-nodes>
		</node>
		<node id="operate" name="operate" title="转化页面">
			<component name="forwardComponent">
				<properties>
					<flow-property name="path" value="/crud/flow/operate.page" />
				</properties>
			</component>
			<next-nodes>
			    <next-node next-node-id="end"></next-node>
			</next-nodes>
		</node>
	</nodes>
</flow>
